{
	"name": "nvmgui",
	"version": "0.0.2",
	"private": true,
	"homepage": "./",
	"build": {
		"appId": "nwlab.nvmgui",
		"mac": {
			"category": "nwlab.nvmgui.idontknow"
		},
		"extends": null,
		"win": {
			"requestedExecutionLevel": "requireAdministrator",
			"target": [
				"portable"
			]
		},
		"portable": {
			"artifactName": "nvmgui.exe"
		}
	},
	"dependencies": {
		"electron-notifications": "^1.0.0"
	},
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"build": "electron-builder --dir"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"electron": "^20.2.0",
		"electron-builder": "^23.3.3"
	}
}
